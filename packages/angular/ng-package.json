{
  "$schema": "node_modules/ng-packagr/ng-package.schema.json",
  "dest": "build",
  "lib": {
    "entryFile": "src/index.ts",
    "umdModuleIds": {
      "@sentry/browser": "Sentry",
      "@sentry/utils": "Sentry.util"
    }
  },
  "whitelistedNonPeerDependencies": [
    "@sentry/browser",
    "@sentry/utils",
    "@sentry/types",
    "tslib",
    "rxjs"
  ],
  "assets": ["README.md", "LICENSE"],
  "keepLifecycleScripts": true
}
